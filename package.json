    "./react": {
      "import": {
        "types": "./dist/react.d.mts",
        "default": "./dist/react.mjs"
      }
    }
  },
  "dependencies": {
    "@std/encoding": "npm:@jsr/std__encoding@^1.0.5",
    "@supabase/supabase-js": "^2.46.1",
    "react": "^18.3.1",
    "valtio": "^2.1.2",
    "valtio-yjs": "^0.6.0",
    "yjs": "^13.6.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "@types/react": "^18.3.12",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "react": "^18.3.1",
    "valtio": "^2.1.2",
    "valtio-yjs": "^0.6.0",
    "yjs": "^13.6.20"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsup"
  }
}
